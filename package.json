{
  "name": "@xch/web-gis-elements",
  "version": "0.6.2",
  "description": "Common components used in GIS packaged in HTML elements.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zodiase/Web-GIS-Elements.git"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run demo:build && npm run build:dist",
    "build:dev": "webpack --config webpack.dev.js --progress --profile --colors",
    "build:dist": "webpack --config webpack.prod.js --progress --profile --colors",
    "watch": "npm run build:dev -- --watch",
    "demo:build": "npm run demo:build:dev",
    "demo:build:dev": "DISTDIR=\"docs/demo/lib/mapview\" npm run build:dev",
    "demo:build:dist": "DISTDIR=\"docs/demo/lib/mapview\" npm run build:dist",
    "demo:build:watch": "DISTDIR=\"docs/demo/lib/mapview\" npm run watch",
    "demo": "node demo-server.js 5000",
    "test:unit": "mocha-webpack --webpack-config webpack.unit-test.js -r jsdom-global/register \"src/**/*.spec.js\"",
    "test:integration": "mochify --allow-chrome-as-root \"test/**/*.spec.js\"",
    "test:dev": "npm run build:dev && npm run test:unit && npm run test:integration",
    "test": "npm run build && npm run test:unit && npm run test:integration"
  },
  "author": "Xingchen Hong",
  "license": "Apache-2.0",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^1.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babili-webpack-plugin": "0.1.2",
    "chai": "^4.1.2",
    "color": "^3.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "jsdom": "^11.8.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.5",
    "lodash.clone": "^4.5.0",
    "lodash.concat": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "mocha": "^5.1.1",
    "mocha-webpack": "^1.1.0",
    "mochify": "^5.6.0",
    "null-loader": "^0.1.1",
    "openlayers": "^4.4.2",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.5",
    "sinon": "^4.5.0",
    "type-check": "^0.3.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-node-externals": "^1.7.2"
  }
}
