{
  "name": "web-gis-elements",
  "version": "0.3.0",
  "description": "Custom elements for Web GIS applications.",
  "repository": {
    "type": "git",
    "url": "https://xhong@bitbucket.org/cybergis/web-gis-components.git"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:dist",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "watch": "webpack --env=dev --progress --profile --colors --watch",
    "demo:build": "npm run demo:build:dev",
    "demo:build:dev": "DISTDIR=\"docs/demo/lib/mapview\" npm run build:dev",
    "demo:build:dist": "DISTDIR=\"docs/demo/lib/mapview\" npm run build:dist",
    "demo:build:watch": "DISTDIR=\"docs/demo/lib/mapview\" npm run watch",
    "demo": "node demo-server.js 5000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Xingchen Hong",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babili-webpack-plugin": "0.0.11",
    "css-loader": "^0.28.7",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "express": "^4.16.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "lodash.clone": "^4.5.0",
    "lodash.concat": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "openlayers": "^4.4.2",
    "type-check": "^0.3.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.7.0"
  }
}
